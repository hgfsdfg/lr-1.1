console.log(division)
function isPrimeDivisor(i){
    let sub, counter = 0
    for(let j=1; j<=i; j++) {
        sub = i
        while (sub > 0) {
            sub -= j
            if (sub == 0) {
                counter++
            }
        }
    }
    console.log("counter = " + counter)
     if (counter == 2) {
    //     console.log("Prime " + i)
        return true
    } 
    else {
        return false
    }
        
}
